{
  "name": "nacho",
  "displayName": "Nacho",
  "description": "Variation of the R markdown language, which allows you to have indented subheadings and highlights within comments",
  "version": "0.8.1",
  "publisher": "nachocab",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nacho",
        "aliases": [
          "nacho",
          "nacho"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nacho",
        "scopeName": "source.nacho",
        "path": "./syntaxes/nacho.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:nacho"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "repository": "git@github.com:nachocab/vscode-nacho-language.git",
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "mocha": "^10.2.0",
    "typescript": "^4.9.4",
    "@types/vscode": "^1.74.0",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {}
}
