{
  "name": "nacho",
  "displayName": "Nacho",
  "description": "Variation of the R markdown language, which allows you to have indented subheadings and highlights within comments",
  "version": "0.8.5",
  "publisher": "nachocab",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nacho",
        "aliases": [
          "nacho",
          "nacho"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nacho",
        "scopeName": "source.nacho",
        "path": "./syntaxes/nacho.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [],
  "main": "./out/src/extension",
  "scripts": {
    "pretest": "npm run compile",
    "compile": "tsc -p ./",
    "compile:watch": "tsc -watch -p ./",
    "test": "mocha",
    "test:watch": "mocha --watch"
  },
  "repository": "git@github.com:nachocab/vscode-nacho-language.git",
  "devDependencies": {
    "@types/vscode": "^1.84.1",
    "@types/mocha": "^10.0.4",
    "@types/node": "^20.9.0",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}
