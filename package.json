{
  "name": "nacho",
  "displayName": "Nacho",
  "description": "Variation of the R markdown language, which allows you to have indented subheadings and highlights within comments",
  "version": "0.6.0",
  "publisher": "nachocab",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nacho",
        "aliases": [
          "nacho",
          "nacho"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nacho",
        "scopeName": "source.nacho",
        "path": "./syntaxes/nacho.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:nacho"
  ],
  "main": "./out/src/extension",
  "dependencies": {},
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "repository": "git@github.com:nachocab/vscode-nacho-language.git",
  "devDependencies": {
    "vscode": "^1.1.36",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.3",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3"
  }
}
